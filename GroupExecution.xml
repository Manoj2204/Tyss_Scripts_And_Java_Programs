<suite thread-count="1" parallel="tests" name="Suite">
	<groups>
		<run>
			<include name="smoke"></include>
		</run>
	</groups>
	<test thread-count="1" parallel="tests" name="Test" preserve-order="false">
		<classes>
			<class name="PracticeTestNG.Demo_Test">
				<methods>
					<exclude name="practice1"></exclude>
				</methods>
			</class>
			<class name="PracticeTestNG.Sample_Test" />
